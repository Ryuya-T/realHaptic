@inproceedings{Tsujilab_Sutskever2014,
abstract = {Deep Neural Networks (DNNs) are powerful models that have achieved excel-lent performance on difficult learning tasks. Although DNNs work well whenever large labeled training sets are available, they cannot be used to map sequences to sequences. In this paper, we present a general end-to-end approach to sequence learning that makes minimal assumptions on the sequence structure. Our method uses a multilayered Long Short-Term Memory (LSTM) to map the input sequence to a vector of a fixed dimensionality, and then another deep LSTM to decode the target sequence from the vector. Our main result is that on an English to French translation task from the WMT-14 dataset, the translations produced by the LSTM achieve a BLEU score of 34.8 on the entire test set, where the LSTM's BLEU score was penalized on out-of-vocabulary words. Additionally, the LSTM did not have difficulty on long sentences. For comparison, a phrase-based SMT system achieves a BLEU score of 33.3 on the same dataset. When we used the LSTM to rerank the 1000 hypotheses produced by the aforementioned SMT system, its BLEU score increases to 36.5, which is close to the previous state of the art. The LSTM also learned sensible phrase and sentence representations that are sensitive to word order and are relatively invariant to the active and the passive voice. Fi-nally, we found that reversing the order of the words in all source sentences (but not target sentences) improved the LSTM's performance markedly, because doing so introduced many short term dependencies between the source and the target sentence which made the optimization problem easier.},
annote = {NULL},
archivePrefix = {arXiv},
arxivId = {1409.3215},
author = {Sutskever, Ilya and Vinyals, Oriol and Le, Quoc V.},
booktitle = {Proceedings of International Conference on Neural Information Processing Systems},
doi = {10.1007/s10107-014-0839-0},
eprint = {1409.3215},
file = {::},
isbn = {1409.3215},
issn = {09205691},
keywords = {machine learning,neural network,seq2seq},
mendeley-tags = {machine learning,neural network,seq2seq},
pages = {3104--3112},
pmid = {2079951},
title = {{Sequence to sequence learning with neural networks}},
year = {2014}
}
@inproceedings{Tsujilab_Kutsuzawa2017,
abstract = {Manipulating objects is one of the main topics in robotics. There exist various types of manipulation called nonprehensile manipulation which do not involve grasping. This paper proposes a method based on sequence-to-sequence (seq2seq) models, which deforms a given trajectory to another trajectory accomplishing nonprehensile manipulation. As the first step to realize it, this paper proposes a method of training seq2seq models to learn identity mapping. By chunking trajectories, seq2seq models can learn to memorize given trajectories and reproduce them while avoiding the gradient exploding problem and increase of memory usage. The results of this study provide useful knowledge for development of neural models on learning trajectory deformation in nonprehensile manipulation.},
annote = {提案手法のチャンク化は，wordpieceモデルに極めて近いと考えられる。},
author = {Kutsuzawa, Kyo and Sakaino, Sho and Tsuji, Toshiaki},
booktitle = {Proceedings of IEEJ International Workshop on Sensing, Actuation, Motion Control, and Optimization},
file = {::},
keywords = {motion planning,neural network,nonprehensile manipulation,sequence-to-sequence model,trajectory deformation},
mendeley-tags = {motion planning,neural network},
title = {{Learning identity mapping of trajectories by sequence-to-sequence model with time series chunking}},
year = {2017}
}
@inproceedings{Tsujilab_Kutsuzawa2018,
abstract = {In order for robots to work in our daily lives, they often have to contact with their environment. They need to control both position and force, while it is a difficult issue when the environment varies. One approach for this issue is to deform trajectories with respect to the environmental variation. Since there may occur various environment fluctuation, we need a general-purpose method for contact motion deformation. We focus on neural networks. This paper proposes a method of deforming trajectories of position and force by using sequence-to-sequence models. The proposed model can adapt to various requirements by only designing a differentiable loss function. The algorithms of deformation are obtained by the proposed model with unsupervised learning. This paper shows an example of implementation in one-dimensional contact motion. This paper shows that the proposed method can reproduce the timing of contact even when the positions of contact objects change.},
author = {Kutsuzawa, Kyo and Sakaino, Sho and Tsuji, Toshiaki},
booktitle = {Proceedings of IEEJ International Workshop on Sensing, Actuation, Motion Control, and Optimization},
file = {::},
keywords = {contact motion,haptics,neural networks,seq2seq,sequence-to-sequence model},
mendeley-tags = {haptics,seq2seq},
title = {{Deformation of Contact Motion by Neural Networks to Adapt for Various Environment Change}},
year = {2018}
}
